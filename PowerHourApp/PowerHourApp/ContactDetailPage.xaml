<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PowerHourApp.ContactDetailPage"
             Padding="50,50,50,50"
             Title="{Binding ContentPageTitle}"
             Icon="button.png">
    <ContentPage.Content>
        <StackLayout>
            <Label HorizontalOptions="Center" Text="{Binding ContentPageText}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>